<%= form_for @order, :url => transfer_admin_order_path(@order), :html => {:id => "transfer-form", :method => "POST"} do |f| %>
	<ul>
	<% @sales.each do |sale| %>
		<li><%= radio_button("order", "authorized_approver_id", sale.id.to_s) %> <%= sale.nickname %><li>
	<% end %>
	</ul>
	<%= f.submit "Transfer" %>
<% end %>